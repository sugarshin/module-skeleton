{
  "name": "module-skeleton",
  "version": "0.0.1",
  "description": "module-skeleton",
  "repository": "git@github.com:sugarshin/module-skeleton.git",
  "main": "index.js",
  "files": [
    "lib",
    "index.js",
    "test",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "start": "coffee -cw -o lib/ src/",
    "test": "mocha --require espower-coffee/guess test/**/*.coffee",
    "compile": "coffee -c -o lib/ src/",
    "build-standalone": "browserify src/module-skeleton.coffee -t coffeeify -s ModuleSkeleton | derequire > build/module-skeleton.js",
    "build-min": "uglifyjs build/module-skeleton.js -m -c warnings=false --comments '/\\/*!/' -o build/module-skeleton.min.js",
    "build": "npm run compile && npm run build-standalone && npm run build-min"
  },
  "engines": {
    "node": "0.10.x"
  },
  "author": "sugarshin",
  "license": "MIT"
}
